{
    "@@locale": "es",
    "counterAppBarTitle": "Contador",
    "@counterAppBarTitle": {
        "description": "Texto mostrado en la AppBar de la página del contador"
    },
    "helloWorld": "¡Hola Mundo desde Tecylab!",
    "@helloWorld": {
        "description": "Texto de bienvenida mostrado en la página del contador"
    },
    "welcomeMessage": "¡Bienvenido, {userName}!",
    "remainingItems": "{count, plural, =0 {No quedan elementos} =1 {Queda 1 elemento} other {{count} elementos restantes}}",
    "@remainingItems": {
        "description": "Mensaje que muestra el número de elementos restantes",
        "placeholders": {
            "count": {
                "type": "int",
                "example": "5"
            }
        }
    },
    "greeting": "{gender, select, male {Hello Mr.} female {Hello Ms.} other {Hello}}",
    "@greeting": {
        "description": "Greeting based on gender",
        "placeholders": {
            "gender": {
                "type": "String",
                "example": "male"
            }
        }
    },
    "escapedMessage": "This is a JSON string with \"quotes\" and \\slashes\\.",
    "formattedDate": "Hoy es {date, date, ::yMMMd}",
    "@formattedDate": {
        "description": "La fecha actual en formato corto",
        "placeholders": {
            "date": {
                "type": "DateTime",
                "example": "2024-12-25T10:00:00Z"
            }
        }
    },
    "formattedNumber": "The price is {compact}",
    "@formattedNumber": {
        "description": "The price formatted as a compact number",
        "placeholders": {
            "price": {
                "type": "double",
                "example": "1234.56"
            }
        }
    }
}